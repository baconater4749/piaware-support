#!/usr/bin/make -f
# -*- makefile -*-

export DH_VERBOSE=1

override_dh_systemd_enable:
	dh_systemd_enable --name=regenerate-ssh-keys systemd/regenerate-ssh-keys.service
	dh_systemd_enable --name=generate-network-config systemd/generate-network-config.service

override_dh_systemd_start:
	dh_systemd_start --no-start

%:
	dh $@ --with=systemd
